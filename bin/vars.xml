            <Step level="6" action="move" from="{{UnzipTo}}\{{GitHubR}}\gw2addon_d3d9_wrapper.pdb" to="{{GW2Dir}}\addons\{{GitHubR}}\gw2addon_d3d9_wrapper.pdb"/>            
